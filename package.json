{
    "name": "bidcars",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "dev:web": "pnpm --filter web dev",
        "dev:api": "pnpm --filter api start:dev",
        "dev": "concurrently -n WEB,API -c blue,green \"pnpm dev:web\" \"pnpm dev:api\""
    },
    "devDependencies": {
        "concurrently": "^9.2.1"
    }
}